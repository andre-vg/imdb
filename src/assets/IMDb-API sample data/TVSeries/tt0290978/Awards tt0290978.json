  "awardsHtml": "<div class=\"awards\">\r\n    <h5>24 wins & 18 nominations</h5>\r\n    <h3>Primetime Emmy Awards</h3>\r\n    <table>\r\n        <tbody>\r\n            <tr>\r\n                <td>\r\n                    <b>2005 Nominee Primetime Emmy</b>\r\n                    <br />\r\n                    Outstanding Made for Television Movie\r\n                    <br />\r\n                    Jon Plowman (executive producer) - Anil Gupta (executive producer) - Ash Atalla (producer) (For episode \"Christmas Special: Part 1 (#3.1)\".)\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>2005 Nominee Primetime Emmy</b>\r\n                    <br />\r\n                    Outstanding Writing for a Miniseries, Movie or a Dramatic Special\r\n                    <br />\r\n                    Ricky Gervais (writer) - Stephen Merchant (writer) (For episode \"Christmas Special: Part 1 (#3.1)\".)\r\n                </td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n    <h3>BAFTA Awards</h3>\r\n    <table>\r\n        <tbody>\r\n            <tr>\r\n                <td>\r\n                    <b>2004 Winner BAFTA TV Award</b>\r\n                    <br />\r\n                    Best Comedy Performance\r\n                    <br />\r\n                    Ricky Gervais (For the 2003 christmas special.)\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>2004 Nominee BAFTA TV Award</b>\r\n                    <br />\r\n                    Best Comedy Performance\r\n                    <br />\r\n                    Martin Freeman (For the 2003 christmas special.)\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>2004 Winner BAFTA TV Award</b>\r\n                    <br />\r\n                    Situation Comedy Award\r\n                    <br />\r\n                    Ash Atalla - Ricky Gervais - Stephen Merchant (For the 2003 christmas special.)\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>2003 Winner BAFTA TV Award</b>\r\n                    <br />\r\n                    Best Comedy Performance\r\n                    <br />\r\n                    Ricky Gervais\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>2003 Winner BAFTA TV Award</b>\r\n                    <br />\r\n                    Situation Comedy Award\r\n                    <br />\r\n                    Ash Atalla - Anil Gupta - Ricky Gervais - Stephen Merchant\r\n                </td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n    <h3>Online Film & Television Association</h3>\r\n    <table>\r\n        <tbody>\r\n            <tr>\r\n                <td>\r\n                    <b>2019 Winner OFTA TV Hall of Fame</b>\r\n                    <br />\r\n                    Television Programs\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>2003 Nominee OFTA Television Award</b>\r\n                    <br />\r\n                    Best Actor in a Comedy Series\r\n                    <br />\r\n                    Ricky Gervais\r\n                </td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n    <h3>Gold Derby Awards</h3>\r\n    <table>\r\n        <tbody>\r\n            <tr>\r\n                <td>\r\n                    <b>2010 Nominee Gold Derby TV Award</b>\r\n                    <br />\r\n                    Comedy Lead Actor of the Decade\r\n                    <br />\r\n                    Ricky Gervais\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>2010 Nominee Gold Derby TV Award</b>\r\n                    <br />\r\n                    Comedy Supporting Actor of the Decade\r\n                    <br />\r\n                    John Krasinski\r\n                </td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n    <h3>Television Critics Association Awards</h3>\r\n    <table>\r\n        <tbody>\r\n            <tr>\r\n                <td>\r\n                    <b>2005 Winner TCA Award</b>\r\n                    <br />\r\n                    Outstanding Achievement in Movies, Miniseries and Specials\r\n                    <br />\r\n                     (For the special.)\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>2004 Nominee TCA Award</b>\r\n                    <br />\r\n                    Outstanding Achievement in Comedy\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>2004 Winner TCA Award</b>\r\n                    <br />\r\n                    Individual Achievement in Comedy\r\n                    <br />\r\n                    Ricky Gervais\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td>\r\n                    <b>2003 Nominee TCA Award</b>\r\n                    <br />\r\n                    Outstanding Achievement in Comedy\r\n                </td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n    <h3>The Webby Awards</h3>\r\n    <table>\r\n        <tbody>\r\n            <tr>\r\n                <td>\r\n                    <b>2005 Nominee Webby Award</b>\r\n                    <br />\r\n                    Websites - Television & Film\r\n                </td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n    <h3>Golden Globes, USA</h3>\r\n    <table>\r\n        <tbody>\r\n            <tr>\r\n                <td>\r\n                    <b>2004 Winner Golden Globe</b>\r\n                    <br />\r\n                    Best Television Series - Comedy or Musical\r\n                </td>\r\n            </tr>